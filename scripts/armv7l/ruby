fcntl64: arg1 == F_GETFD || arg1 == F_GETFL || arg1 == F_SETFL
fstat64: 1
ftruncate64: 1
getcwd: 1
getdents64: 1
getrandom: 1
ioctl: return ENOSYS
lseek: 1
lstat64: 1
